{
    "manifest_version": 3,
    "name": "GitHubPDF",
    "description": "View PDFs within Google PDF Reader And GitHub",
    "version": "1.0",
    "permissions": [
        "tabs",
        "activeTab",
        "storage"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_title": "GitHubPDFs",
        "default_icon": "./icons/icon2.png"
    },
    "background.service_worker": {
        "scripts": ["background.js"]
    }
}